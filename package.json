{
  "name": "desafio-final-m6",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:api": "nodemon ./server/dev.ts",
    "dev:front": "parcel serve ./client/index.html --dist-dir public",
    "dev": "npx concurrently --names \"FRONTEND,SERVER\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn dev:front\" \"yarn dev:api\"",
    "build:front": "parcel build ./client/index.html --dist-dir public",
    "build:api": "tsc ./server/index.ts",
    "build": "yarn build:front && build:api",
    "start": "node index.js"
  },
  "dependencies": {
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "firebase": "^9.10.0",
    "firebase-admin": "^11.0.1",
    "nanoid": "^4.0.0",
    "parcel": "^2.7.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^18.7.23"
  }
}
